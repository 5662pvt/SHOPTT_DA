@model IEnumerable<SHOPTT_DA.Models.ThuongHieu>

<p class="lead">Thương Hiệu Sản Phẩm </p>
<div class="list-group">
    @foreach (var item in Model)
    {
        <a href="@Url.Action("SPTheoTH", "Sneaker", new { id = item.MaThuongHieu})" class="list-group-item">
            @item.TenThuongHieu
        </a>
    }
</div>
